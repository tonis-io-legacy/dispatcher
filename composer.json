{
    "name": "tonis/dispatcher",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me"
        }
    ],
    "description": "Tonis\\Dispatcher is a light-weight, HHVM compatible, and dependency free dispatching library.",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "zendframework/zend-diactoros": "^1.0",
        "phpunit/phpunit": "^4.0",
        "scrutinizer/ocular": "^1.1.1",
        "squizlabs/php_codesniffer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tonis\\Dispatcher\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tonis\\Dispatcher\\": [
                "test/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
